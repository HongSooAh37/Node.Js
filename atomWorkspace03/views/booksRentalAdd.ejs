<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Free Bootstrap Admin Template : Two Page</title>
    <!-- BOOTSTRAP STYLES-->
    <link href="/assets/css/bootstrap.css" rel="stylesheet" />
    <!-- FONTAWESOME STYLES-->
    <link href="/assets/css/font-awesome.css" rel="stylesheet" />
    <!-- CUSTOM STYLES-->
    <link href="/assets/css/custom.css" rel="stylesheet" />
    <!-- GOOGLE FONTS-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />
</head>
<body>
    <div id="wrapper">
        <div class="navbar navbar-inverse navbar-fixed-top">

        <!-- [menu] include-->
        <% include ./module/menu.ejs %>

        <!-- body start -->
        <div id="page-wrapper">
            <div id="page-inner">
                <div class="row">
                    <div class="col-md-12">
                        <h2>Books Rental insert</h2>
                    </div>
                </div>
                <!-- /. ROW  -->
                <hr />
                <!-- /. ROW  -->
                <!-- BODY MIDDLE START -->
                  <div class="row">
				    <div class="col-md-8 col-md-offset-0">
				      <form class="form-horizontal" role="form" method='post' action='/rent/booksRentalAdd'>
				        <fieldset>
				          <!-- Form Name -->
				          <legend>Books Rental</legend>
				          <!-- Text input-->
				          <div class="form-group">
				            <label class="col-sm-2 control-label" for="textinput">도서번호</label>
				            <div class="col-sm-10">
				              <input type="text" name="booksNumber" placeholder="booksNumber" class="form-control">
				            </div>
				          </div>
				          <!-- Text input-->
      					   <div class="form-group">
					            <label class="col-sm-2 control-label" for="textinput">회원번호</label>
						            <div class="col-sm-10">
						              <input id="membersNumber" name="membersNumber" type="text" placeholder="State" class="form-control">
						            </div>
				          </div>

				          <!-- Text input-->
				          <div class="form-group">
				            <label class="col-sm-2 control-label" for="textinput">대여 시작일</label>
				            <div class="col-sm-4">
				              <input id="booksRentStartDate" name="booksRentStartDate" type="date" placeholder="State" class="form-control">
				            </div>
				          </div>

				          <div class="form-group">
				            <label class="col-sm-2 control-label" for="textinput">반납 종료일</label>
				            <div class="col-sm-4">
				              <input id="booksRentEndDate" name="booksRentEndDate" type="date" placeholder="Address Line 2" class="form-control">
				            </div>
				          </div>

                  <div class="form-group">
                     <label class="col-sm-2 control-label" for="textinput">대여료</label>
                       <div class="col-sm-10">
                         <input id="booksRentPay" name="booksRentPay" type="text" placeholder="State" class="form-control">
                       </div>
                 </div>
				          <div class="form-group">
				            <div class="col-sm-offset-2 col-sm-10">
				              <div class="pull-right">
				                <input type="reset" class="btn btn-default" value="reset">
				                <input type="submit" class="btn btn-primary" value="rental">
				              </div>
				            </div>
				          </div>
				        </fieldset>
				      </form>
				    </div><!-- /.col-lg-12 -->
	</div>
<!-- /.row -->
<!-- BODY MIDDLE END -->
	</div>
                <hr />
            </div>
            <!-- /. PAGE INNER  -->
        </div>
        <!-- /. PAGE WRAPPER  -->
    </div>
    <!-- /. WRAPPER  -->
    <!-- SCRIPTS -AT THE BOTOM TO REDUCE THE LOAD TIME-->
    <!-- JQUERY SCRIPTS -->
    <script src="/assets/js/jquery-1.10.2.js"></script>
    <!-- BOOTSTRAP SCRIPTS -->
    <script src="/assets/js/bootstrap.min.js"></script>
    <!-- METISMENU SCRIPTS -->
    <script src="/assets/js/jquery.metisMenu.js"></script>
    <!-- CUSTOM SCRIPTS -->
    <script src="/assets/js/custom.js"></script>
</body>
</html>
